# LLM Configuration
LLM_PROVIDER=chutes
LLM_MODEL=unsloth/Llama-3.2-3B-Instruct
LLM_BASE_URL=https://llm.chutes.ai
LLM_TEMPERATURE=0.1
LLM_TOP_P=0.9
LLM_TIMEOUT_MS=60000

# Chutes API Key (required)
# Get your API key from https://chutes.ai/app/api
CHUTES_API_KEY=your_api_key_here

# Test Configuration
MAX_TICKS_PER_STEP=25
ARIA_SNAPSHOT_MAX_CHARS=8000
SHORT_TEXT_MAX_CHARS=2000

# Test credentials (example)
TEST_USERNAME=testuser
TEST_PASSWORD=testpass123

# Debug
DEBUG=false
LOG_LEVEL=info
